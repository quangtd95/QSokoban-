<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.quangtd.qsokoban">

    <application
        android:name="com.quangtd.qsokoban.common.MyApplication"
        android:icon="@mipmap/sokobon_trans"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sokobon_trans"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.quangtd.qsokoban.ui.screen.game.GameActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".ui.screen.level.LevelActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".ui.screen.home.HomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>